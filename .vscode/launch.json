{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "shakespere - sonnets",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}/shakespere",
            "program": "${workspaceFolder}/shakespere/.get-sonnets.js"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "sitchin - enki",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}/sitchin",
            "program": "${workspaceFolder}/sitchin/.transform.js"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "epic of gilgamesh",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}/eog",
            "program": "${workspaceFolder}/eog/.transform.js"
        }
    ]
}
